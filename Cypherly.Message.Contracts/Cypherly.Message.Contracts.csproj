<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>Cypherly.Message.Contracts</PackageId>
        <Version>1.0.0</Version>
        <Authors>KristianJBorgwarth</Authors>
        <PackageDescription>Shared message contracts for Cypherly services.</PackageDescription>
        <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <IncludeSymbols>false</IncludeSymbols>
        <IncludeSource>false</IncludeSource>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MassTransit.Abstractions" Version="8.5.0" />
    </ItemGroup>

</Project>
